{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 1,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {        
          "type": "os",
          "style": "diamond",  
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "‚ï≠‚îÄÓÇ≤",
          "properties": {
            "windows": "Óò™"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "background": "#3465a4",
          "foreground": "#ffffff",
          "properties": {
            "template": " Óóø {{ .Path }} ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "background": "#ffffff",
          "foreground": "#193549",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "",
            "template": " Ôêà {{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "background": "#86BBD8",
          "foreground": "#FFD700",
          "properties": {
            "root_icon": " ÔÇ≠ ",
            "template": " \uf0e7 "
          }
        },
        {
          "style": "powerline",
          "type": "exit",
          "powerline_symbol": "ÓÇ∞",
          "background": "#ff8080",
          "foreground": "#ffffff",
          "properties": {
            "template": " Óàè {{ if gt .Code 0 }}{{ .Meaning }}{{ else }}\uf42e{{ end }} "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true,
            "template": " ÔÆ´{{ .FormattedMs }} "
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "npm_icon": " Óúû ",
            "template": " Ô¢ò {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
            "yarn_icon": " Ôòö "
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#ffffff",
          "foreground": "#bd2929",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": " Óú∏ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#7c5ea3",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": " Óúå {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6c82ff",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": " ÓòÜ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
          }
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#0383d3",
          "foreground": "#ffffff",
          "properties": {
            "template": " Ôíâ {{ .Name }} "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "trailing_diamond": "ÓÇ¥",
          "background": "#ffffff",
          "foreground": "#000000",
          "properties": {
            "template": " {{ .CurrentDate | date .Format }} ",
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "template": "‚ï∞‚îÄüöÄ"
          }
        }
      ]
    }
  ]
}
