{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",

  "final_space": false,
  "osc99": true,

  "console_title": false,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}",

  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "ÓäÖ "
  },

  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ∞",
          "properties": {
            "postfix": "",
            "windows": "Ô°± ",
            "macos": "ÔÖπ ",
            "ubuntu": "Ôåõ "
          }
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "background": "#0383d3",
          "foreground": "#ffffff",
          "properties": {
            "prefix": " Ôíâ ",
            "postfix": " "
          }
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#172055",
          "properties": {
            "prefix": " Ô°ö CPU:",
            "postfix": "% ",
            "template": "{{ round .PhysicalPercentUsed .Precision }}"
          }
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#4d4181",
          "properties": {
            "prefix": " Ô°ö MEM:",
            "postfix": "GB ",
            "template": "{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#575656",
          "properties": {
            "prefix": " ÔÆ´",
            "threshold": 0,
            "style": "round"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "background": "#86BBD8",
          "foreground": "#FFD700",
          "properties": {
            "postfix": "administrador ",
            "root_icon": "ÔÇ≠ ",
            "template": " ÔÉß "
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "invert_powerline": false,
          "background": "#ff8080",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": false,
            "prefix": " Óàè "
          }
        }
      ]
    },

    {
      "type": "prompt",
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "npm_icon": "Óó∫ ",
            "yarn_icon": "Ôòö ",
            "template": "Ô¢ò {{ .Full }}"
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#ffffff",
          "foreground": "#bd2929",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": "Óú∏ {{ .Full }}"
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#7c5ea3",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": "Óúå {{ .Full }}"
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6c82ff",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": "Óàµ {{ .Full }}"
          }
        },
        {
          "type": "ruby",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6c82ff",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": "Óûë {{ .Full }}"
          }
        },
        {
          "type": "angular",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "#6c82ff",
          "foreground": "#ffffff",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": "Óùì {{ .Full }}"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "trailing_diamond": "ÓÇ¥",
          "leading_diamond": "ÓÇ≤",
          "background": "#ffffff",
          "foreground": "#000000",
          "properties": {
            "prefix": "Ôôè ",
            "time_format": "15:04:05"
          }
        }
      ]
    },

    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "‚ï≠‚îÄÓÇ≤",
          "trailing_diamond": "ÓÇ∞",
          "background": "#3465a4",
          "foreground": "#ffffff",
          "properties": {
            "style": "full",
            "template": "ÔÅº {{ .Path }}",
            "folder_separator_icon": "/",
            "windows_registry_icon": "ÓÇ±"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "background": "#ffffff",
          "foreground": "#193549",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }} #FFEB3B {{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }} #FFCC80 {{ end }}",
            "{{ if gt .Ahead 0 }} #ffffff {{ end }}",
            "{{ if gt .Behind 0 }} #B388FB {{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }} {{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }} {{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }} {{ if gt .StashCount 0 }} Ôöí {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        }
      ]
    },

    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#3465a4",
          "properties": {
            "prefix": "",
            "postfix": " ",
            "text": "‚ï∞‚îÄüöÄ"
          }
        }
      ]
    }
  ]
}
